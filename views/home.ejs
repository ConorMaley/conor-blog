<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <nav>
        <ul>
            <li><a href="/">Home</a></li>
            <li><a href="/about">About</a></li>
            <li><a href="/contact">Contact</a></li>
        </ul>
    </nav>
    <main>
        <% posts.forEach(post => { %>
            <article>
                <h2><a href="/blog/<%= post.fileAlias %>"><%= post.title %></a></h2>
                <p><%- post.content %></p>
            </article>
        <% }) %>
    </main>
</body>
</html>